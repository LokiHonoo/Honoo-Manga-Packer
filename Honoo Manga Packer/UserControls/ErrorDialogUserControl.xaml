<UserControl x:Class="Honoo.MangaPacker.UserControls.ErrorDialogUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:hui="https://github.com/lokihonoo/honooui.wpf"
             xmlns:local="clr-namespace:Honoo.MangaPacker.UserControls"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:system="clr-namespace:System;assembly=mscorlib"
             xmlns:vm="clr-namespace:Honoo.MangaPacker.ViewModels"
             Width="430"
             Height="390"
             d:Background="White"
             d:DesignHeight="370"
             d:DesignWidth="400"
             mc:Ignorable="d">
    <UserControl.DataContext>
        <vm:ErrorDialogUserControlViewModel />
    </UserControl.DataContext>
    <ListView ItemsSource="{Binding Settings.ErrorMessages}" />
</UserControl>
